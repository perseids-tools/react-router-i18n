{
  "name": "react-router-i18n",
  "version": "0.1.0",
  "description": "Opinionated internationalization (i18n) library using React Router",
  "main": "build/index.js",
  "keywords": [
    "react-router",
    "i18n",
    "internationalization",
    "perseids"
  ],
  "scripts": {
    "start": "node scripts/start.js",
    "deploy": "gh-pages -d demo",
    "predeploy": "PUBLIC_URL='/react-router-i18n/' yarn run demo",
    "build": "node scripts/build.js",
    "demo": "node scripts/demo.js && cp demo/index.html demo/404.html",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint --ext=js --ext=jsx ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perseids-tools/react-router-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/perseids-tools/react-router-i18n/issues"
  },
  "homepage": "https://github.com/perseids-tools/react-router-i18n",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.3.0 || ^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.4",
    "@svgr/webpack": "4.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.0",
    "bfj": "7.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "3.0.0",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "3.11.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "file-loader": "4.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "8.1.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-pnp-resolver": "1.2.1",
    "jest-resolve": "24.8.0",
    "jest-watch-typeahead": "^0.3.1",
    "mini-css-extract-plugin": "0.7.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.8.6",
    "resolve": "1.11.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.3.0",
    "url-loader": "2.0.1",
    "webpack": "4.35.3",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
