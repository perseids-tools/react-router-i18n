{
  "name": "react-router-i18n",
  "version": "0.2.1",
  "description": "Opinionated internationalization (i18n) library using React Router",
  "main": "build/index.js",
  "keywords": [
    "react-router",
    "i18n",
    "internationalization",
    "perseids"
  ],
  "scripts": {
    "start": "node scripts/start.js",
    "deploy": "gh-pages -d demo",
    "predeploy": "PUBLIC_URL='/react-router-i18n/' yarn run demo",
    "build": "node scripts/build.js",
    "demo": "node scripts/demo.js && cp demo/index.html demo/404.html",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint --ext=js --ext=jsx ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perseids-tools/react-router-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/perseids-tools/react-router-i18n/issues"
  },
  "homepage": "https://github.com/perseids-tools/react-router-i18n",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.3.0 || ^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@svgr/webpack": "5.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "bfj": "7.0.2",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "file-loader": "6.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "9.0.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "4.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "jest-pnp-resolver": "1.2.1",
    "jest-resolve": "25.1.0",
    "jest-watch-typeahead": "^0.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "10.1.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.13.1",
    "resolve": "1.15.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "url-loader": "4.0.0",
    "webpack": "4.42.1",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
